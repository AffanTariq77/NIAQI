{
  "expo": {
    "name": "NIAQI",
    "slug": "NIAQI",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "niaqi",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.niaqi.app",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow NIAQI to access your photos",
        "NSCameraUsageDescription": "Allow NIAQI to use your camera"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "package": "com.niaqi.app",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "niaqi"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "updates": {
      "enabled": true,
      "url": "https://u.expo.dev/0121ef5f-93e1-4417-9619-c07dac6babc8",
      "fallbackToCacheTimeout": 0
    },
    "platforms": ["ios", "android"],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0121ef5f-93e1-4417-9619-c07dac6babc8"
      }
    },
    "runtimeVersion": "1.0.0"
  }
}
